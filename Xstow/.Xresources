! One Dark colorscheme: https://github.com/joshdick/onedark.vim/blob/master/term/One%20Dark.Xresources

*.foreground:   #ABB2BF
*.background:   #282C34
*.cursorColor:  #ABB2BF

! black
*.color0:       #5C6370
*.color8:       #4B5263

! red
*.color1:       #E06C75
*.color9:       #BE5046

! green
*.color2:       #98C379
*.color10:      #98C379

! yellow
*.color3:       #E5C07B
*.color11:      #D19A66

! blue
*.color4:       #61AFEF
*.color12:      #61AFEF

! magenta
*.color5:       #C678DD
*.color13:      #C678DD

! cyan
*.color6:       #56B6C2
*.color14:      #56B6C2

! white
*.color7:       #ABB2BF
*.color15:      #3E4452


! rxvt-unicode specific settings
!URxvt.font: xft:Source\ Code\ Pro\ for\ Powerline:size=11:antialias=true:hinting=true,xft:PowerlineSymbols
URxvt.font: xft:Mononoki:size=13:antialias=true:hinting=true,xft:PowerlineSymbols
URxvt.scrollBar:          False
URxvt.depth:              32
URxvt.intensityStyles:    True

! It''s very important to tune this variable before using any of the Powerline fonts.
! See https://github.com/powerline/fonts/issues/176
URxvt.letterSpace: 0


! Colors for bold, italic and underlined text. Mostly for the man pages
URxvt.colorIT: #000000
URxvt.colorBD: #40C4FF
URxvt.colorUL: #FF4081

! Fade the terminal a bit if not focused
URxvt*fading: 7
